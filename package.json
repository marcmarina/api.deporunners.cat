{
  "name": "api.deporunners.cat",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "serve": "node dist/index.js",
    "start": "nodemon --exec ts-node --files src/index.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "seed": "ts-node seed.ts",
    "lint": "eslint . --ext .ts",
    "mongo:start": "docker run --rm -d -p 27017:27017 --name deporunners_mongo mongo:latest",
    "prepare": "husky install",
    "docker:build": "docker build . -t marcmarina/deporunners-api",
    "docker:push": "docker push marcmarina/deporunners-api"
  },
  "lint-staged": {
    "**/*.*(ts|js)": [
      "prettier --write"
    ]
  },
  "author": "Marc Marina",
  "license": "ISC",
  "repository": "https://github.com/marcmarina/api.deporunners.cat",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^14.0.14",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.1.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@sentry/node": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "css-inliner": "^2.0.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "excel4node": "^1.7.2",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.28",
    "nodemailer": "^6.4.13",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pug": "^3.0.0",
    "stripe": "^8.111.0",
    "zod": "^3.5.1"
  }
}
