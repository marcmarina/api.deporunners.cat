{
  "name": "api.deporunners.cat",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/marcmarina/api.deporunners.cat",
  "license": "ISC",
  "author": "Marc Marina",
  "main": "dist/index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "services:start": "docker-compose -f docker-compose.yml up -d",
    "services:stop": "docker-compose -f docker-compose.yml down",
    "start": "turbo run start",
    "test": "turbo run test",
    "verify": "turbo run verify"
  },
  "dependencies": {
    "turbo": "^1.9.3"
  },
  "devDependencies": {
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8"
  }
}
